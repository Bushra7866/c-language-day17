#include<stdio.h>
int main()
{
	int i,c=0;
	printf("enter a value");
	scanf("%d",&i);
	if(i==1)
	{
		printf("%d is neither prime nor composite",i);
	}
	else if(i==2)
	{
		printf("%d is an even-prime number",i);
	}
	else if(i%2==0){
		printf("%d is not a prime number",i);
}
		else{
		printf("%d is prime number",i);
	}
}
